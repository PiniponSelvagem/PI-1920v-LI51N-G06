<table id="series-table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Original Name</th>
            <th>User Score</th>
            <th id="th-score">Set Score</th>
            <th id="th-button"></th>
        </tr>
    </thead>
    <tbody id="items">
        {{#each series}}
        <tr>
            <td>{{name}}</td>
            <td>{{original_name}}</td>
            <td>{{userScore}}</td>
            <td><input type="number" id="score" min="1" max="10" step="1" value="1"/><button id="btn-score-serie" value={{id}}>set score</button></td>
            <td><button id="btn-delete-serie" value={{id}}>delete</button></td>
        </tr>
        {{/each}}
    </tbody>
</table>